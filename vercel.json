{
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://peepers.vercel.app"
  },
  "functions": {
    "src/app/api/ml/webhook/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/ml/sync/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/ml/webhook",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/ml/(.*)",
      "destination": "/api/ml/$1"
    },
    {
      "source": "/api/products(.*)",
      "destination": "/api/products$1"
    }
  ]
}
