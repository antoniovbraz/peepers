{
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://peepers.vercel.app"
  },
  "functions": {
    "src/app/api/webhook/mercado-livre/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/sync/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/webhook/mercado-livre",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/webhook/(.*)",
      "destination": "/api/webhook/$1"
    },
    {
      "source": "/api/products(.*)",
      "destination": "/api/products$1"
    }
  ]
}
