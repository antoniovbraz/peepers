{
  "functions": {
    "src/app/api/ml/webhook/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/ml/sync/route.ts": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/ml/sync",
      "schedule": "0 */2 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/ml/webhook",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
